@model IEnumerable<NoteWebApp.Areas.Lab.Models.BinaryFileVO>

@{
    ViewBag.Title = "SimpleFileUpload";
}

<h2>SimpleFileUpload</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.BINARY_FILE_ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FILE_NAME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FILE_EXT)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BINARY_DATA)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FILE_SIZE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IS_DELETED)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CREATED_DATE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MODIFIED_DATE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DELETED_DATE)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.BINARY_FILE_ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FILE_NAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FILE_EXT)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BINARY_DATA)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FILE_SIZE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IS_DELETED)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CREATED_DATE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MODIFIED_DATE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DELETED_DATE)
        </td>
    </tr>
}

</table>
